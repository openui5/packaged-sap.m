<mvc:View
	controllerName="sap.ui.demo.cart.controller.OrderCompleted"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">
	<Page title="{i18n>ordercompletedTitle}">
		<content>
			<FormattedText htmlText="{i18n>ordercompletedText}" />
		</content>
		<footer>
			<Bar>
				<contentRight>
					<Button id="returnToShopButton"
							type="Emphasized"
							text="{i18n>returntoshopButtonText}"
							press="onReturnToShopButtonPress" />
				</contentRight>
			</Bar>
		</footer>
	</Page>
</mvc:View>
